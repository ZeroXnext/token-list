{
  "name": "tokenlist-builder",
  "packageManager": "yarn@4.11.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "core:add": "yarn workspace @tokenlist-builder/core add",
    "cli:add": "yarn workspace @tokenlist-builder/cli add",
    "core:rm": "yarn workspace @tokenlist-builder/core remove",
    "cli:rm": "yarn workspace @tokenlist-builder/cli remove",
    "build": "turbo build"
  },
  "devDependencies": {
    "turbo": "^2.6.0"
  }
}
